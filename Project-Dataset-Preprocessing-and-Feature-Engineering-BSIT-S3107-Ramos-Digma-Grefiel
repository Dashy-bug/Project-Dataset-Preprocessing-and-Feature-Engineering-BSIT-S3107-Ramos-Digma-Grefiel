   "source": [
    "#CELL 7\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def boxplot_by_category(df, category_col, value_col, title, top_n=20):\n",
    "    # Work on a clean copy of just the needed columns\n",
    "    d = df[[category_col, value_col]].copy()\n",
    "\n",
    "    # Clean category labels\n",
    "    d[category_col] = d[category_col].astype(str).str.strip()\n",
    "\n",
    "    # Force value column to numeric (turns bad strings like \"N/A\" into NaN)\n",
    "    d[value_col] = pd.to_numeric(d[value_col], errors=\"coerce\")\n",
    "\n",
    "    # Drop rows that can't be plotted\n",
    "    d = d.dropna(subset=[category_col, value_col])\n",
    "\n",
    "    # Optional: keep only top categories to avoid unreadable plots\n",
    "    if top_n is not None:\n",
    "        top = d[category_col].value_counts().head(top_n).index\n",
    "        d = d[d[category_col].isin(top)]\n",
    "\n",
    "    # Plot\n",
    "    plt.figure(figsize=(10, 6))\n",
    "    d.boxplot(column=value_col, by=category_col, grid=True)\n",
    "    plt.title(title)\n",
    "    plt.suptitle(\"\")\n",
    "    plt.xlabel(category_col)\n",
    "    plt.ylabel(value_col)\n",
    "    plt.xticks(rotation=45, ha=\"right\")\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "    # Quick sanity check prints\n",
    "    non_numeric_count = pd.to_numeric(df[value_col], errors=\"coerce\").isna().sum()\n",
    "    print(f\"[Info] Rows plotted: {len(d)}\")\n",
    "    print(f\"[Info] Non-numeric or missing values in '{value_col}' (coerced to NaN): {non_numeric_count}\")\n",
    "\n",
    "# OPTIONAL: run this if you want to see examples of the “bad” values\n",
    "def show_non_numeric_examples(df, col, n=15):\n",
    "    coerced = pd.to_numeric(df[col], errors=\"coerce\")\n",
    "    bad = df.loc[coerced.isna() & df[col].notna(), col].astype(str).head(n).tolist()\n",
    "    print(f\"Examples of non-numeric values in '{col}':\")\n",
    "    print(bad)\n",
    "\n",
    "\n"
   ]
  },
