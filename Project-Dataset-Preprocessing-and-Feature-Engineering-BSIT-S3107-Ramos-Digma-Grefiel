     "text": [
      "No product column found (Product_ID / Product_Name / etc.). Skipping product boxplots.\n"
     ]
    }
   ],
   "source": [
    "#CELL 8\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "df = final_df.copy()\n",
    "\n",
    "# Find product key\n",
    "product_key = None\n",
    "for k in [\"Product_ID\", \"Product_Name\", \"Product\", \"SKU\", \"Item_Name\"]:\n",
    "    if k in df.columns:\n",
    "        product_key = k\n",
    "        break\n",
    "\n",
    "if not product_key:\n",
    "    print(\"No product column found (Product_ID / Product_Name / etc.). Skipping product boxplots.\")\n",
    "else:\n",
    "    # Keep only products with enough transactions (so boxplots are meaningful)\n",
    "    MIN_TX = 20\n",
    "    counts = df[product_key].value_counts()\n",
    "    keep = counts[counts >= MIN_TX].index\n",
    "\n",
    "    d = df[df[product_key].isin(keep)].copy()\n",
    "\n",
    "    # If still too many products, keep top 10 by frequency\n",
    "    top_products = d[product_key].value_counts().head(10).index\n",
    "    d = d[d[product_key].isin(top_products)]\n",
    "\n",
    "    # Satisfaction by Product\n",
    "    if \"Satisfaction_Score\" in d.columns:\n",
    "        plt.figure(figsize=(12, 6))\n",
    "        d.boxplot(column=\"Satisfaction_Score\", by=product_key, grid=True)\n",
    "        plt.title(f\"Satisfaction Score by {product_key} (Top 10, min {MIN_TX} tx)\")\n",
    "        plt.suptitle(\"\")\n",
    "        plt.xlabel(product_key)\n",
    "        plt.ylabel(\"Satisfaction_Score\")\n",
    "        plt.xticks(rotation=30, ha=\"right\")\n",
    "        plt.show()\n",
    "\n",
    "    # Recommend by Product\n",
    "    if \"Likelihood_to_Recommend\" in d.columns:\n",
    "        plt.figure(figsize=(12, 6))\n",
    "        d.boxplot(column=\"Likelihood_to_Recommend\", by=product_key, grid=True)\n",
    "        plt.title(f\"Likelihood to Recommend by {product_key} (Top 10, min {MIN_TX} tx)\")\n",
    "        plt.suptitle(\"\")\n",
    "        plt.xlabel(product_key)\n",
    "        plt.ylabel(\"Likelihood_to_Recommend\")\n",
    "        plt.xticks(rotation=30, ha=\"right\")\n",
    "        plt.show()\n"
   ]
  },
  {
